<?xml version="1.0" encoding="utf-8"?>
<selector  xmlns:android="http://schemas.android.com/apk/res/android">
    <item android:state_checked="true">
        <shape>
            <solid
                android:color="#ECECEC">
            </solid>
            <corners
                android:radius="5dp" >
            </corners>
            <stroke android:color="@color/bg_black_green"
                android:width="1dp">
            </stroke>
        </shape>
    </item>
    <item android:state_checked="false">
        <shape>
            <solid
                android:color="#ECECEC">
            </solid>
            <corners
                android:radius="5dp" >
            </corners>
            <stroke android:color="#ECECEC"
                android:width="1dp">
            </stroke>
        </shape>
    </item>
</selector>